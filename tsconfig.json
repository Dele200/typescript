{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "node",
        "target": "ESNext",
        "sourceMap": true,
        "outDir": "dist",
        "esModuleInterop": true,
        "strict": true,
        "noImplicitAny": true, 
        "resolveJsonModule": true,
        "lib": ["ESNext", "DOM"],
        "strictNullChecks": true
        },
    "include": ["src/**/*"]
}